<div class="controls-FormattedTextBox controls-TextBox controls-PhoneTextBox controls-TextBox__size__{{size || 'default'}}">
   <div class="controls-FormattedTextBox__fieldWrapper controls-TextBox__wrapper">
      <div class="controls-FormattedTextBox__field js-controls-FormattedTextBox__field controls-TextBox__field{{enabled ? '' : ' ws-hidden'}}" contenteditable="{{enabled}}">{{_maskTemplateFn(_modelForMaskTpl)|sanitize}}</div>
      <a href="tel:{{srcText}}" class="controls-PhoneTextBox__link controls-Link ws-hover-target{{enabled ? ' ws-hidden'}}">{{(text || '&nbsp;')|sanitize}}</a>
   </div>
</div>