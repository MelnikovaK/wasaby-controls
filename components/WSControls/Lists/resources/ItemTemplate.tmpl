<div class="controls-ListView__item js-controls-ListView__item {{ itemData.selected ? 'controls-ListView__item__selected' }} {{itemData.hovered ? 'controls-ListView__item__hovered' }} {{ multiSelected ? 'controls-ListView__item__multiSelected' }} {{ className }} "
     data-hash="{{ projItem.getHash() }}"
     event:onclick="_onItemClick(projItem)"
     event:onmousemove="_mouseMove(index)">
    <ws:partial template="{{contentTemplate}}" scope="{{...}}" attributes="{{ attributes }}"></ws:partial>
</div>
