<div class="controls-ImportCustomizer-ColumnBinding-View__grid__column">
   <ws:WSControls.Buttons.MenuButton
      class="controls-ImportCustomizer-ColumnBinding-View__grid__column__menu"
      name="{{column.menuConf.namePrefix + column.field}}"
      caption="{{column.menuConf.caption}}"
      pickerClassName="controls-ImportCustomizer-ColumnBinding-View__grid__column__menu__picker"
      items="{{column.menuConf.items}}"
      selectedKeys="{{column.selectedMenuId ? [column.selectedMenuId] : []}}"
      idProperty="{{column.menuConf.idProperty}}"
      displayProperty="{{column.menuConf.displayProperty}}"
      parentProperty="{{column.menuConf.parentProperty}}"
      handlers="{{column.menuConf.handlers}}"
   />
</div>