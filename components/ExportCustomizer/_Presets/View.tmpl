<div class="controls-ExportCustomizer-Presets-View">


   <SBIS3.CONTROLS:SelectorButton
      className="controls-ExportCustomizer-Presets-View__button"
      name="controls-ExportCustomizer-Presets-View__button"

      withoutCross="{{true}}"

      _handlers="{{undefined}}"

      ___multiselect="{{false}}"
      ___selectMode="floatArea"
      ___chooserMode="dialog"
      idProperty="id"
      displayProperty="title"

      ___items="{{ _items }}"

      dictionaries="{{ [{
         'template': 'SBIS3.CONTROLS/ListView',
         'dialogOptions': {
            'className': 'controls-ExportCustomizer-Presets-View__picker',
            'border': false,
            '___closeButton': false,
            'direction': 'bottom',
            'isStack': false,
            '___fixed': false,
            '___fitWindow': false,
            'offset': {'x':0, 'y':0}
         },
         'componentOptions': {
            'className': 'controls-ExportCustomizer-Presets-View__list',
            'items': _items,
            'idProperty': 'id',
            'displayProperty': 'title',
            'itemsActionsInItemContainer': true,
            'footerTpl': 'tmpl!SBIS3.CONTROLS/ExportCustomizer/_Presets/tmpl/footer'
         }
      }] }}"
      >
      <ws:defaultCaption>
         <ws:if data="{{dialogTitle}}">{{dialogTitle}}</ws:if><ws:else>{[НастройщикЭкспорта@@Настройка экспорта]}</ws:else>
      </ws:defaultCaption>
      <ws:tooltip>
         <ws:if data="{{presetsTooltip}}">{{presetsTooltip}}</ws:if><ws:else>{[НастройщикЭкспорта@@Выбрать другой шаблон]}</ws:else>
      </ws:tooltip>
   </SBIS3.CONTROLS:SelectorButton>


</div>
