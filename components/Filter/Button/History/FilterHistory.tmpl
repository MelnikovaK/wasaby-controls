<div class="controls-filterButton__history-area ws-hidden">
   <span class="controls-filterButton__secondary-title">{[Ранее отбирались]}</span>
   <div class="controls-filterButton__footerAreas-wrapper controls-filterButton__footerAreas-maxHeight">
      <SBIS3.CONTROLS:ListView
         name="historyView"
         className="controls-ListView__withoutMarker controls-filterButton__historyView"
         itemsActions="{{[]}}"
         idProperty="id"
         itemsDragNDrop=""
      >
         <ws:itemContentTpl>
            <ws:String>tmpl!SBIS3.CONTROLS/Filter/Button/historyItemContentTpl</ws:String>
         </ws:itemContentTpl>
         <ws:handlers>
            <ws:Object>
               <ws:onChangeHoveredItem>
                  <ws:Function>SBIS3.CONTROLS/Filter/Button/History:prototype._onChangeHoverHistoryItem</ws:Function>
               </ws:onChangeHoveredItem>
            </ws:Object>
         </ws:handlers>
      </SBIS3.CONTROLS:ListView>
   </div>
   <SBIS3.CONTROLS:Commands:CommandsSeparator
      name="toggleHistory"
      command="toggleHistory"
   />
</div>