<div class="controls-FilterPanelItem{{item.template === 'js!SBIS3.CONTROLS.FilterPanelBoolean' ? ' controls-FilterPanelItem__boolean'}}">
   <ws:if data="{{item.template === 'js!SBIS3.CONTROLS.FilterPanelBoolean'}}">
      <ws:SBIS3.CONTROLS.FilterPanelBoolean hasMarkup="{{ true }}" caption="{{ item.caption }}" checked="{{ 'value'|mutable }}" value="{{ 'value'|mutable }}" textValue="{{ 'textValue'|mutable }}" name="{{ item.id }}" />
   </ws:if>
   <ws:else>
      <ws:SBIS3.CONTROLS.Spoiler
         expanded="{{ item.expanded }}"
         caption="{{ item.caption }}"
         properties="{{ item.properties }}"
         name="{{ item.id }}"
         rightPartTitleTpl="tmpl!SBIS3.CONTROLS.FilterPanelItem/resources/FilterPanelItemSpoilerRightPartTitleTemplate"
         contentTpl="{{ item.template }}" >
      </ws:SBIS3.CONTROLS.Spoiler>
   </ws:else>
</div>