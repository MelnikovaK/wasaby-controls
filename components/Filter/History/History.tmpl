<div class="controls-History">
    <div class="controls-History-title">{[Ранее отбирались]}</div>
    <SBIS3.CONTROLS:Filter:HistoryView name="ReportFavoriteView"
                                       itemsActions="{{ [_unFavoriteAction] }}"
                                       historyId="{{ (historyId + '-favorite') }}"
                                       idProperty="{{idProperty}}"
                                       displayProperty="{{displayProperty}}"
                                       footerTpl=""
                                       className="controls-History-view controls-History-favoritesView"
                                       itemsSortMethod="{{ _favoriteSortMethod }}">
    </SBIS3.CONTROLS:Filter:HistoryView>
    <SBIS3.CONTROLS:Filter:HistoryView name="ReportHistoryView"
                                       itemsActions="{{ [_favoriteAction] }}"
                                       historyId="{{ historyId }}"
                                       idProperty="{{idProperty}}"
                                       displayProperty="{{displayProperty}}"
                                       footerTpl=""
                                       className="controls-History-view">
    </SBIS3.CONTROLS:Filter:HistoryView>
    <SBIS3.CONTROLS:Action:OpenDialogAction name="editFavorite" initializingWay="local" dialogComponent="SBIS3.CONTROLS/Filter/FavoriteEditDialog"/>
</div>