<div class="controls-FilterPanelItem">
   <ws:if data="{{item.template === 'js!SBIS3.CONTROLS.FilterPanelBoolean'}}">
      <ws:SBIS3.CONTROLS.FilterPanelBoolean hasMarkup="{{ true }}" caption="{{item.caption}}" filter="{{'filter'|mutable}}"/>
   </ws:if>
   <ws:else>
      <ws:SBIS3.CONTROLS.Spoiler
              expanded="{{ item.expanded }}"
              caption="{{ item.caption }}"
              properties="{{ item.properties }}"
              name="{{ item.id }}"
              titleTpl="tmpl!SBIS3.CONTROLS.FilterPanel/resources/FilterPanelSpoilerTitleTemplate"
              contentTpl="{{ item.template }}" >
      </ws:SBIS3.CONTROLS.Spoiler>
   </ws:else>
</div>