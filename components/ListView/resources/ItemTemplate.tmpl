<div class="controls-ListView__item {{_options.itemsHover ? ' controls-ListView__item__withHover' : ' controls-ListView__item__withoutHover'}} js-controls-ListView__item
    {{selectedKey==item.getId() ? 'controls-ListView__item__selected'}}
    {{ className }}
    {{drawHiddenGroup ? ' ws-hidden' : ''}}"
     data-hash="{{ projItem.getHash() }}"
     data-id="{{ item.getId() + '' }}">
    <ws:partial template="{{itemContent}}" scope="{{...}}" attributes="{{ attributes }}"></ws:partial>
</div>
