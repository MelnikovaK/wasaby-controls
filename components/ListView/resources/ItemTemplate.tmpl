<div class="controls-ListView__item js-controls-ListView__item
    {{selectedKey==item.getId() ? 'controls-ListView__item__selected'}}
    {{ className }}
    {{drawHiddenGroup ? ' ws-hidden' : ''}}
    {{noHover ? '' : ' controls-ListView__hoveredItem__withHover'}}"
     data-hash="{{ projItem.getHash() }}"
     data-id="{{ item.getId() + '' }}">
    <ws:partial template="{{itemContent}}" scope="{{...}}" attributes="{{ attributes }}"></ws:partial>
</div>
