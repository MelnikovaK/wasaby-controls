<div class="controls-ItemsToolbar__editActions ws-hidden">
   <SBIS3.CONTROLS:Button:IconButton class="controls-ItemActions-button"
                                 command="commitEdit"
                                 commandArgs="{{[true]}}"
                                 allowChangeEnable="{{false}}"
                                 className="controls-IconButton__round-border-24"
                                 icon="sprite:icon-size icon-Yes icon-done"
         tooltip="{[Сохранить]}"/>
   <ws:WSControls.Buttons.Button class="controls-ItemsToolbar-closeButton controls-IconButton controls-Button__closeButton" command="cancelEdit"
         allowChangeEnable="{{false}}"
         tooltip="{[Отмена]}"/>
</div>