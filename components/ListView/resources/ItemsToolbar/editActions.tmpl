<div class="controls-ItemsToolbar__editActions ws-hidden">
   <SBIS3.CONTROLS:Button:IconButton class="controls-IconButton__round-border-24 controls-IconButton__round"
                                 command="commitEdit"
                                 commandArgs="{{[true]}}"
                                 allowChangeEnable="{{false}}"
                                 icon="sprite:icon-size icon-Yes icon-done"
         tooltip="{[Сохранить]}"></SBIS3.CONTROLS:Button:IconButton>
   <SBIS3.CONTROLS:Button:IconButton class="controls-Button__closeButton" command="cancelEdit"
         allowChangeEnable="{{false}}"
         tooltip="{[Отмена]}"></SBIS3.CONTROLS:Button:IconButton>
</div>