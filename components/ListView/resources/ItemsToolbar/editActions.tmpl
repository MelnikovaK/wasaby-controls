<div class="controls-ItemsToolbar__editActions ws-hidden">
   <ws:SBIS3.CONTROLS.IconButton class="controls-IconButton__round-border-24 controls-IconButton__round"
                                 command="commitEdit"
                                 commandArgs="{{[true]}}"
                                 allowChangeEnable="{{false}}"
                                 icon="sprite:icon-size icon-Yes icon-done"
         tooltip="{[Сохранить]}"></ws:SBIS3.CONTROLS.IconButton>
   <ws:SBIS3.CONTROLS.IconButton class="controls-Button__closeButton" command="cancelEdit"
         allowChangeEnable="{{false}}"
         tooltip="{[Отмена]}"></ws:SBIS3.CONTROLS.IconButton>
</div>