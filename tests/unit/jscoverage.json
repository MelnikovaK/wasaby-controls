{
  "execWin": "tests/unit/bin/jscoverage/jscoverage-server.exe",
  "execNix": "/usr/bin/jscoverage-server",
  "docRoot": "",
  "params": {
    "verbose": true,
    "ip-address": "0.0.0.0",
    "port": 667,
    "no-instrument": [
      "/sbis3-ws/",
      "/node_modules/",
      "/tests/",
      ".test.js"
    ],
    "report-dir": "tests/unit/artifacts/jscoverage-report"
  },
  "webdriver": {
    "host": "localhost",
    "path": "/jscoverage.html?tests/unit/via-webdriver.html"
  },
  "checkerTimeout": 15000,
  "reportTimeout": 2000
}
