<bl_module bl_module_library="sbis-bl-service300" id="6f8333a3-aa5f-404e-ae9f-6fa42dcdc349" name="Бизнес-логика" package="Платформа" responsible="Керимов В.А." version="1.00">

  <description/>

  <depends>
    <bl_module id="16779777-d6ee-4d93-bd86-83dbd7ce03b4" name="Python"/>
    <bl_module id="6fc02564-c885-4b66-b8e8-727bff7a8436" name="Работа с БД"/>
