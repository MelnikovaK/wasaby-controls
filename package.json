{
   "name": "sbis3-controls",
   "version": "3.18.600",
   "repository": {
      "type": "git",
      "url": "git@git.sbis.ru:root/sbis3-controls"
   },
   "files": [
      "Controls/*",
      "File/*"
   ],
   "scripts": {
      "start": "node app",
      "build": "grunt build",
      "rebuild": "grunt rebuild",
      "test": "node node_modules/ws-unit-testing/mocha --amd test-isolated",
      "test-debug": "node node_modules/ws-unit-testing/mocha --amd %NODE_DEBUG_OPTION% test-isolated",
      "coverage": "node node_modules/ws-unit-testing/cover --amd test-isolated",
      "lint": "node ./node_modules/eslint/bin/eslint --color --no-eslintrc -c=.eslintrc -f=stylish ."
   },
   "config": {
      "ws": "/",
      "wsModules": {
         "View": "sbis3-ws/View",
         "Core": "sbis3-ws/Core"
      },
      "resources": "./",
      "tests": "tests/unit",
      "test_server_port": 1025,
      "test_report": "artifacts/xunit-report.xml",
      "test_url": {
         "scheme": "http",
         "host": "localhost",
         "port": 1025,
         "path": "",
         "query": "reporter=XUnit"
      },
      "htmlCoverageReport": "artifacts/coverage/index.html",
      "jsonCoverageReport": "artifacts/coverage.json"
   },
   "nyc": {
      "include": [
         "components/**/*.js",
         "Controls/**/*.js",
         "File/**/*.js"
      ],
      "reporter": [
         "text",
         "html"
      ],
      "cache": false,
      "eager": true,
      "report-dir": "./artifacts/coverage"
   },
   "dependencies": {
      "typescript": "^2.9.1"
   },
   "devDependencies": {
      "@tensor-corp/eslint-config": "^2.0.3",
      "async": "^2.6.1",
      "autoprefixer": "^8.6.5",
      "body-parser": "^1.18.3",
      "cookie-parser": "^1.4.3",
      "eslint": "^5.5.0",
      "express": "^4.16.3",
      "grunt": "^0.4.5",
      "grunt-contrib-clean": "0.6.0",
      "grunt-contrib-copy": "0.7.0",
      "grunt-contrib-jshint": "0.11.0",
      "grunt-contrib-less": "1.0.0",
      "grunt-contrib-watch": "0.6.1",
      "grunt-express-server": "0.4.19",
      "gulp": "^4.0.0",
      "gulp-less": "^3.5.0",
      "gulp-livereload": "^3.8.1",
      "humanize": "0.0.9",
      "istanbul": "0.4.x",
      "less": "^2.7.3",
      "load-grunt-config": "0.16.0",
      "merge": "1.2.0",
      "minimatch": "^3.0.3",
      "postcss": "^6.0.23",
      "progress": "^2.0.0",
      "requirejs": "2.1.18",
      "serve-static": "1.11.x",
      "ws-unit-testing": "git+https://git.sbis.ru/ws/unit-testing.git#rc-3.18.600",
      "typescript": "2.5.3",
      "ws-data": "git+http://git.sbis.ru/ws/data.git#3.18.600/bugfix/d-t-pj"
   }
}
