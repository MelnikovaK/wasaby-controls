<div class="PageWrap" style="padding-top: 50px;">
   <div class="PageBlock">
      <div class="PageBlock__title"></div>
      <div class="PageBlock__componentWrap ws-flexbox" style="justify-content: space-evenly; align-items: baseline">
         <Controls.Input.Date.LinkView
               prevArrowVisibility="{{true}}"
               nextArrowVisibility="{{true}}"
         />
         <Controls.Input.Date.LinkView
               bind:startValue="_startValue"
               bind:endValue="_endValue"
               prevArrowVisibility="{{true}}"
               nextArrowVisibility="{{true}}"

         />
         <Controls.Input.Date.LinkView
               bind:startValue="_startValue"
               bind:endValue="_endValue"
               prevArrowVisibility="{{true}}"
               nextArrowVisibility="{{true}}"

         />
         <Controls.Input.Date.LinkView
               bind:startValue="_startValue"
               bind:endValue="_endValue"
               viewMode="link" styleMode="secondary"
               clearButtonVisibility="{{true}}"
         />
         <Controls.Input.Date.LinkView
               startValue="{{_startValue}}"
               endValue="{{_endValue}}"
               viewMode="link" styleMode="info"
         />
         <Controls.Input.Date.LinkView
               startValue="{{_startValue}}"
               endValue="{{_endValue}}"
               viewMode="label"
         />
      </div>
      <div class="PageBlock__componentWrap ws-flexbox" style="justify-content: space-evenly; align-items: baseline; padding-top: 30px;">
         <Controls.Input.Date.LinkView
               prevArrowVisibility="{{true}}"
               nextArrowVisibility="{{true}}"
               bind:startValue="_startValue"
               bind:endValue="_endValue"
               styleMode="info"
         />
      </div>
   </div>
</div>
