<div>
    <div class="control-Lookup-demo-block">
        <div class="control-Lookup-demo-title">
            Lookup single select
        </div>
        <div>
            Тест базовой линии
            <Controls.Input.Lookup
                    bind:value="_value"
                    bind:selectedKeys="_selectedKeys"
                    source="{{_source}}"
                    placeholder="Введите имя"
                    searchParam="title"
                    keyProperty="id"
                    displayProperty="title"
                    attr:class="control-Lookup-demo-Lookup">
                <ws:navigation source="page" view="page">
                    <ws:sourceConfig pageSize="{{2}}" page="{{0}}" mode="totalCount"/>
                </ws:navigation>
                <ws:suggestTemplate templateName="Controls-demo/Input/Lookup/Suggest/SuggestTemplate"/>
            </Controls.Input.Lookup>
        </div>
    </div>
    <div class="control-Lookup-demo-block">
        <div class="control-Lookup-demo-title">
            Lookup single select readOnly
        </div>
        <div>
            Тест базовой линии
            <Controls.Input.Lookup
                    bind:value="_value1"
                    bind:selectedKeys="_selectedKeys1"
                    source="{{_source}}"
                    placeholder="Введите имя"
                    searchParam="title"
                    readOnly="{{true}}"
                    keyProperty="id"
                    attr:class="control-Lookup-demo-Lookup">
                <ws:navigation source="page" view="page">
                    <ws:sourceConfig pageSize="{{2}}" page="{{0}}" mode="totalCount"/>
                </ws:navigation>
                <ws:suggestTemplate templateName="Controls-demo/Input/Lookup/Suggest/SuggestTemplate"/>
            </Controls.Input.Lookup>
        </div>
    </div>
    <div class="control-Lookup-demo-block">
        <div class="control-Lookup-demo-title">
            Lookup bold items
        </div>
        <div>
            Тест базовой линии
            <Controls.Input.Lookup
                    bind:value="_value2"
                    bind:selectedKeys="_selectedKeys2"
                    source="{{_source}}"
                    placeholder="Введите имя"
                    searchParam="title"
                    keyProperty="id"
                    attr:class="control-Lookup-demo-Lookup">
                <ws:navigation source="page" view="page">
                    <ws:sourceConfig pageSize="{{2}}" page="{{0}}" mode="totalCount"/>
                </ws:navigation>
                <ws:suggestTemplate templateName="Controls-demo/Input/Lookup/Suggest/SuggestTemplate"/>
                <ws:itemTemplate>
                    <ws:partial template="tmpl!Controls/Input/Lookup/ItemTemplate" style="bold"/>
                </ws:itemTemplate>
            </Controls.Input.Lookup>
        </div>
    </div>
    <div class="control-Lookup-demo-block">
        <div class="control-Lookup-demo-title">
            Lookup accent items
        </div>
        <div>
            Тест базовой линии
            <Controls.Input.Lookup
                    bind:value="_value3"
                    bind:selectedKeys="_selectedKeys3"
                    source="{{_source}}"
                    placeholder="Введите имя"
                    searchParam="title"
                    keyProperty="id"
                    attr:class="control-Lookup-demo-Lookup">
                <ws:navigation source="page" view="page">
                    <ws:sourceConfig pageSize="{{2}}" page="{{0}}" mode="totalCount"/>
                </ws:navigation>
                <ws:suggestTemplate templateName="Controls-demo/Input/Lookup/Suggest/SuggestTemplate"/>
            </Controls.Input.Lookup>
        </div>
    </div>
    <div class="control-Lookup-demo-block">
        <div class="control-Lookup-demo-title">
            Lookup primary items
        </div>
        <div>
            Тест базовой линии
            <Controls.Input.Lookup
                    bind:value="_value4"
                    bind:selectedKeys="_selectedKeys4"
                    source="{{_source}}"
                    placeholder="Введите имя"
                    searchParam="title"
                    keyProperty="id"
                    attr:class="control-Lookup-demo-Lookup">
                <ws:navigation source="page" view="page">
                    <ws:sourceConfig pageSize="{{2}}" page="{{0}}" mode="totalCount"/>
                </ws:navigation>
                <ws:suggestTemplate templateName="Controls-demo/Input/Lookup/Suggest/SuggestTemplate"/>
                <ws:itemTemplate>
                    <ws:partial template="tmpl!Controls/Input/Lookup/ItemTemplate" style="primary" size="l"/>
                </ws:itemTemplate>
            </Controls.Input.Lookup>
        </div>
    </div>
</div>