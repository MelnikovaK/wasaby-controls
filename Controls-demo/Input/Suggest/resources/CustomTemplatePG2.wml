<div class="controls-demo__SuggestPG-item">
    <ws:if data="{{itemData.getPropValue(itemData.item, 'city')}}">
        <div><b>{{itemData.getPropValue(itemData.item, 'city')}}</b></div>
        <div><i>The best city in the World</i></div>
    </ws:if>
    <ws:if data="{{itemData.getPropValue(itemData.item, 'name')}}">
        <div><b>{{itemData.getPropValue(itemData.item, 'name')}}</b></div>
        <div><i>The best name in the World</i></div>
    </ws:if>
</div>