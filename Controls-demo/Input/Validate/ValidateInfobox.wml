<div style="width:250px; display: flex; padding: 20px;">
    <Controls.Validate.FormController>
        <ws:content>
            <div>
                <Controls.Validate.Input   name="InputValidate" attr:style="margin: 30px" >
                    <ws:validators>
                        <ws:Function value="{{_value }}">Controls/Validate/Validators/IsINN</ws:Function>
                        <ws:Function value="{{_value }}">Controls/Validate/Validators/IsEmail</ws:Function>
                    </ws:validators>
                    <ws:content>
                        <Controls.Input.Text
                                name="textBox"
                                bind:value="_value"
                                placeholder="{{_placeholder}}"
                                tagStyle="primary"
                                validationErrors="{{_validationErrors}}"
                        />
                    </ws:content>
                </Controls.Validate.Input>
                <Controls.Validate.Input   name="InputValidate2" attr:style="margin: 30px"  >
                    <ws:validators>
                        <ws:Function value="{{_value2 }}">Controls/Validate/Validators/IsINN</ws:Function>
                    </ws:validators>
                    <ws:content>
                        <Controls.Input.Text
                                name="textBox2"
                                bind:value="_value2"
                                placeholder="{{_placeholder}}"
                                tagStyle="primary"
                                validationErrors="{{_validationErrors}}"
                        />
                    </ws:content>
                </Controls.Validate.Input>
            </div>
        </ws:content>
    </Controls.Validate.FormController>
    <Controls.Button caption="c" on:click="_paste()"/>
</div>
