<div style="padding: 20px;">
<div class="validateDemo_block">
      <span id="info_focusout" class="validateDemo_header">Validate/Input (Валидация происходит по focusOut)</span>
      <div class="validateDemo_content">
                <Controls.Validate.Input name="InputValidate" attr:class="ControlsDemo-ValidateInfobox__inputValidate">
                    <ws:validators>
                        <ws:Function value="{{_valueEmail}}">Controls/Validate/Validators/IsEmail</ws:Function>
                    </ws:validators>
                    <ws:content>
                        <Controls.input:Text
                                name="textBox"
                                bind:value="_valueEmail"
                                placeholder="Введите Email"/>
                    </ws:content>
                </Controls.Validate.Input>
                <Controls.Validate.Input name="InputValidate2" attr:class="ControlsDemo-ValidateInfobox_inputValidate2">
                    <ws:validators>
                        <ws:Function value="{{_value3 }}">Controls/Validate/Validators/IsINN</ws:Function>
                    </ws:validators>
                    <ws:content>
                        <Controls.input:Number
                                name="textBox"
                                bind:value="_value3"
                                placeholder="Введите ИНН"
                        />
                    </ws:content>
                </Controls.Validate.Input>
                 <Controls.Validate.Input name="InputValidate3" attr:class="ControlsDemo-ValidateInfobox_inputValidate3">
                    <ws:validators>
                         <ws:Function value="{{_value6 }}">Controls-demo/Input/Validate/Validator</ws:Function>
                    </ws:validators>
                    <ws:content>
                        <Controls.input:Text
                                name="textBox"
                                bind:value="_value6"
                                placeholder="Введите код"
                        />
                    </ws:content>
                </Controls.Validate.Input>
      </div>
    <span id="info_focusout" class="validateDemo_header">Валидация на стековой панели</span>
    <div class="validateDemo_content">
                <Controls.buttons:Button name="Panel" on:click="_openPanel()" caption="Открыть StackPanel" attr:class="ControlsDemo-ValidateInfobox_panel"/>
                <Controls.Popup.Opener.Stack name="opener"
                                             template="Controls-demo/Input/Validate/Text">
                </Controls.Popup.Opener.Stack>
    </div>
    <Controls.Validate.FormController name="formController">
        <ws:content>
            <div>
                <div class="validateDemo_header">Validate/Controller(Кнопка запускает процесс валидации)</div>
            <div class="validateDemo_content">
                <Controls.Validate.Controller  name="ControllerValidate">
                    <ws:validators>
                        <ws:Function value="{{_value2}}" >Controls/Validate/Validators/IsRequired</ws:Function>
                    </ws:validators>
                    <ws:content>
                        <Controls.input:Text
                                name="textBox2"
                                bind:value="_value2"
                                placeholder="Email"/>
                    </ws:content>
                </Controls.Validate.Controller>
                <Controls.Validate.Controller  name="ControllerValidate2" attr:class="ControlsDemo-ValidateInfobox_infobox">
                    <ws:validators>
                        <ws:Function value="{{_value1}}">Controls/Validate/Validators/IsEmail</ws:Function>
                    </ws:validators>
                    <ws:content>
                        <Controls.Input.Text
                                name="textBox5"
                                bind:value="_value1"
                                placeholder="Email"/>
                    </ws:content>
                </Controls.Validate.Controller>
                 <Controls.Validate.Controller  name="ControllerValidate3">
                    <ws:validators>
                        <ws:Function value="{{_value5 }}">Controls/Validate/Validators/IsEmail</ws:Function>
                    </ws:validators>
                    <ws:content>
                        <Controls.input:Text
                                name="textBox5"
                                bind:value="_value5"
                                placeholder="Email"/>
                    </ws:content>
                </Controls.Validate.Controller>
                 <Controls.Validate.Controller  readOnly="{{true}}" name="ControllerValidate4" attr:class="ControlsDemo-ValidateInfobox_readonly">
                    <ws:validators>
                        <ws:Function value="{{_value7 }}">Controls/Validate/Validators/IsEmail</ws:Function>
                    </ws:validators>
                    <ws:content>
                        <Controls.Input.Text
                                attr:style="width:400px"
                                name="textBox5"
                                bind:value="_value7"
                                placeholder="Email"/>
                    </ws:content>
                </Controls.Validate.Controller>
            </div>
        </ws:content>
    </Controls.Validate.FormController>
    <Controls.buttons:Button caption="Submit"  name="updateButton" on:click="_clickHandler()" attr:class="ControlsDemo-ValidateInfobox_submit" />
</div>
