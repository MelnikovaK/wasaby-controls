<div class="popup-page">
    <Controls.popup:Dialog name="dialog"
                                  template="Controls-demo/Popup/Opener/resources/DialogTpl">
    </Controls.popup:Dialog>
    <Controls.popup:Stack name="stack"
                                 template="Controls-demo/Popup/Opener/resources/StackTemplate">
    </Controls.popup:Stack>
<div>
        <Controls.buttons:Button name="button1" caption="Open Stack" on:click="openStack()"/>
</div>
    <div>
    <Controls.buttons:Button name="button2" caption="Open Maximized Stack" on:click="openMaximizedStack()"/>
    </div>
    <div>
        <Controls.buttons:Button name="button4" caption="Open Modal Stack" on:click="openModalStack()"/>
    </div>
    <div>
        <Controls.buttons:Button name="button3" caption="Open Dialog" on:click="openDialog()"/>
    </div>
    <div>
        <Controls.buttons:Button name="button5" caption="Open Modal Dialog" on:click="openModalDialog()"/>
    </div>
</div>