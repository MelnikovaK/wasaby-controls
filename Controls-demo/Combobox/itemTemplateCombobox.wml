<ws:partial template="Controls/dropdown:ItemTemplate"
            itemData="{{itemData}}"
            marker="{{false}}"
            multiLine="{{true}}">
   <ws:contentTemplate>
      <div class="{{!itemData.emptyText ? 'ControlsDemo-ComboboxVDOM__myTempalte'}}">
         {{itemData.getPropValue(itemData.item, 'text')}}
         <div class="ControlsDemo-ComboboxVDOM__comment">
            {{itemData.getPropValue(itemData.item, 'comment')}}
         </div>
      </div>
   </ws:contentTemplate>
</ws:partial>