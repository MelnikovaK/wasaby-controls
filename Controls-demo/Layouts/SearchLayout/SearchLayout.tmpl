<Controls.Container.Filter attr:class="controls-demo__FilterLayout" filterButtonSource="{{_filterSource}}" historyId="123">
    <ws:content>
        <div>
            <Controls.Container.Search
                    attr:class="controls-demo__SearchLayout"
                    searchParam="title">
                <ws:content>
                    <div class="controls-demo__SearchLayout_content">
                        <div class="controls-demo__Layouts_content">
                            <div class="controls-demo__Layouts_filterComponents">
                                <Controls.Input.Search bind:value="_searchValue"/>
                                <ws:Controls.Container.FilterButton>
                                    <ws:content>
                                        <Controls.Filter.Button filterTemplate="Controls-demo/Layouts/SearchLayout/FilterButtonTemplate/FilterButtonTemplate">
                                        </Controls.Filter.Button>
                                    </ws:content>
                                </ws:Controls.Container.FilterButton>
                            </div>
                            <div class="controls-demo__LayoutInner">
                                <Controls.Container.List source="{{_dataSource}}" searchParam="title">
                                    <ws:content>
                                        <div>
                                            <Controls.List idProperty="id" name="list">
                                                <ws:emptyTemplate>
                                                    Не найдено
                                                </ws:emptyTemplate>
                                            </Controls.List>
                                        </div>
                                    </ws:content>
                                    <ws:navigation source="page" view="page">
                                        <ws:sourceConfig pageSize="{{20}}" page="{{0}}" mode="totalCount"/>
                                    </ws:navigation>
                                </Controls.Container.List>
                            </div>
                        </div>
                    </div>
                </ws:content>
            </Controls.Container.Search>
        </div>
    </ws:content>
</Controls.Container.Filter>
