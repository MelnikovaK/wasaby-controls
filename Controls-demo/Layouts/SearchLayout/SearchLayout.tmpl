<Controls.Container.Filter attr:class="controls-demo__FilterLayout" filterButtonSource="{{_filterSource}}" historyId="123" fastFilterSource="{{_fastFilterData}}">
    <ws:content>
        <div>
            <Controls.Container.Search
                    attr:class="controls-demo__SearchLayout"
                    searchParam="firstName">
                <ws:content>
                    <div class="controls-demo__SearchLayout_content">
                        <div class="controls-demo__Layouts_content">
                            <div class="controls-demo__Layouts_filterComponents">
                                <Controls.Container.Input.Search>
                                    <ws:content>
                                        <Controls.Input.Search bind:value="_searchValue"/>
                                    </ws:content>
                                </Controls.Container.Input.Search>
                                <Controls.Container.Filter.Fast>
                                    <ws:content>
                                        <Controls.Filter.Fast bind:selectedKeys="_value"/>
                                    </ws:content>
                                </Controls.Container.Filter.Fast>
                                <Controls.Container.Filter.Button>
                                    <ws:content>
                                        <Controls.Filter.Button viewMode="twoColumn">
                                           <ws:items>
                                              <ws:Array>
                                                 <ws:Object id="firstName" value="123" resetValue="">
                                                    <ws:editor>
                                                       <Controls.Filter.Panel.Editor.Text>
                                                          <ws:content>
                                                             <Controls.Input.Text></Controls.Input.Text>
                                                          </ws:content>
                                                       </Controls.Filter.Panel.Editor.Text>
                                                    </ws:editor>
                                                 </ws:Object>
                                                 <ws:Object id="1" value="123" resetValue="{{true}}">
                                                    <ws:editor>
                                                       <Controls.Filter.Panel.Editor.Boolean>
                                                          <ws:content>
                                                             <Controls.Button caption="click me"
                                                                              style="linkMain"></Controls.Button>
                                                          </ws:content>
                                                       </Controls.Filter.Panel.Editor.Boolean>
                                                    </ws:editor>
                                                 </ws:Object>
                                                 <ws:Object id="2" value="{{_value}}" resetValue="{{1}}">
                                                    <ws:editor>
                                                       <Controls.Filter.Panel.Editor.Keys>
                                                          <ws:content>
                                                             <Controls.Input.Dropdown source="{{sourceDropdown}}"
                                                                                      bind:selectedKeys="_value"
                                                                                      keyProperty="key"></Controls.Input.Dropdown>
                                                          </ws:content>
                                                       </Controls.Filter.Panel.Editor.Keys>
                                                    </ws:editor>
                                                 </ws:Object>
                                                 <ws:Object id="3" value="{{_value}}" resetValue="{{1}}">
                                                    <ws:editor>
                                                       <Controls.Filter.Panel.Editor.Text>
                                                          <ws:content>
                                                             <div>"LOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOONG"</div>
                                                          </ws:content>
                                                       </Controls.Filter.Panel.Editor.Text>
                                                    </ws:editor>
                                                 </ws:Object>
                                              </ws:Array>
                                           </ws:items>
                                        </Controls.Filter.Button>
                                    </ws:content>
                                </Controls.Container.Filter.Button>

                               <Controls.Filter.Button>
                                  <ws:items>
                                     <ws:Array>
                                        <ws:Object id="firstName" value="123" resetValue="">
                                           <ws:editor>
                                              <Controls.Filter.Panel.Editor.Text>
                                                 <ws:content>
                                                    <Controls.Input.Text></Controls.Input.Text>
                                                 </ws:content>
                                              </Controls.Filter.Panel.Editor.Text>
                                           </ws:editor>
                                        </ws:Object>
                                        <ws:Object id="1" value="123" resetValue="{{true}}">
                                           <ws:editor>
                                              <Controls.Filter.Panel.Editor.Boolean>
                                                 <ws:content>
                                                    <Controls.Button caption="click me"
                                                                     style="linkMain"></Controls.Button>
                                                 </ws:content>
                                              </Controls.Filter.Panel.Editor.Boolean>
                                           </ws:editor>
                                        </ws:Object>
                                        <ws:Object id="2" value="{{_value}}" resetValue="{{1}}">
                                           <ws:editor>
                                              <Controls.Filter.Panel.Editor.Keys>
                                                 <ws:content>
                                                    <Controls.Input.Dropdown source="{{sourceDropdown}}"
                                                                             bind:selectedKeys="_value"
                                                                             keyProperty="key"></Controls.Input.Dropdown>
                                                 </ws:content>
                                              </Controls.Filter.Panel.Editor.Keys>
                                           </ws:editor>
                                        </ws:Object>
                                        <ws:Object id="3" value="{{_value}}" resetValue="{{1}}">
                                           <ws:editor>
                                              <Controls.Filter.Panel.Editor.Text>
                                                 <ws:content>
                                                    <div>"LOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOOONG"</div>
                                                 </ws:content>
                                              </Controls.Filter.Panel.Editor.Text>
                                           </ws:editor>
                                        </ws:Object>
                                     </ws:Array>
                                  </ws:items>
                               </Controls.Filter.Button>
                            </div>
                            <div class="controls-demo__LayoutInner">
                                <Controls.Container.List searchParam="firstName" source="{{_dataSource}}">
                                    <ws:content>
                                        <div>
                                            <Controls.List idProperty="id" name="list" >
                                                <ws:itemTemplate>
                                                    <ws:partial template="tmpl!Controls/List/SimpleList/ItemTemplate"
                                                                itemData="{{itemTemplate.itemData}}"
                                                    >
                                                        <ws:contentTemplate>
                                                            <div style="padding: 4px; display: flex; justify-content: space-between">
                                                                <div style="width: 200px">{{itemTemplate.itemData.item.get('firstName')}}</div>
                                                                <div style="font-size: 13px; color: darkblue">{{itemTemplate.itemData.item.get('lastName')}}</div>
                                                                <div style="font-size: 13px; color: limegreen">{{itemTemplate.itemData.item.get('id')}}</div>
                                                            </div>
                                                        </ws:contentTemplate>
                                                    </ws:partial>
                                                </ws:itemTemplate>
                                                <ws:emptyTemplate>
                                                    Не найдено
                                                </ws:emptyTemplate>
                                            </Controls.List>
                                        </div>
                                    </ws:content>
                                    <ws:navigation source="page" view="page">
                                        <ws:sourceConfig pageSize="{{20}}" page="{{0}}" mode="totalCount"/>
                                    </ws:navigation>
                                </Controls.Container.List>
                            </div>
                        </div>
                    </div>
                </ws:content>
            </Controls.Container.Search>
        </div>
    </ws:content>
</Controls.Container.Filter>
