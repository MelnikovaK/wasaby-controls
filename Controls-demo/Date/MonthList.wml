<div style="margin: 50px;" >
   <div>month: {{_month}}</div>
   <div>startValue: {{_startValue}}</div>
   <div>endValue: {{_endValue}}</div>
   <div>selectionProcessing: {{_selectionProcessing}}</div>
   <div>selectionBaseValue: {{_selectionBaseValue}}</div>
   <div>selectionHoveredValue: {{_selectionHoveredValue}}</div>
   <Controls.calendar:MonthList
      attr:style="margin: 10px; border: red;"
      calendarSource="{{_source}}"
      bind:startValue="_startValue"
      bind:endValue="_endValue"
      bind:month="_month"
      bind:selectionProcessing="_selectionProcessing"
      bind:selectionHoveredValue="_selectionHoveredValue"
      bind:selectionBaseValue="_selectionBaseValue"
   >
      <ws:dayTemplate>
         <ws:partial template="wml!Controls/Date/MonthView/dayTemplate"
                     value="{{dayTemplate.value}}"
         >
            <ws:contentTemplate>
               <ws:partial template="{{_dayTemplate}}" value="{{dayTemplate.value}}"/>
            </ws:contentTemplate>
         </ws:partial>
      </ws:dayTemplate>
   </Controls.calendar:MonthList>
   <Controls.buttons:Button caption="_setEndValue" style="buttonAdd" on:click="_setEndValue()"/>
   <Controls.buttons:Button caption="_setselectionProcessing" style="buttonAdd" on:click="_setSelectionProcessing()"/>
   <Controls.buttons:Button caption="_setSelection" style="buttonAdd" on:click="_setSelection()"/>
</div>