<div class="demo-Suggest">
    <div class="demo-SuggestWrapper">
        <h3>Suggest</h3>
        <Controls.Input.Suggest
                bind:value="_suggestValue"
                displayProperty="text">
            <ws:suggestTemplate>
                <Controls.Container.Suggest.List searchParam="title" source="{{_suggestSource}}">
                    <ws:navigation source="page" view="page">
                        <ws:sourceConfig pageSize="{{2}}" page="{{0}}" mode="totalCount"/>
                    </ws:navigation>
                    <ws:content>
                        <Controls:List
                                displayProperty="title"
                                idProperty="id">
                        </Controls:List>
                    </ws:content>
                </Controls.Container.Suggest.List>
            </ws:suggestTemplate>
            <ws:minSearchLength>
                <ws:Number>3</ws:Number>
            </ws:minSearchLength>
        </Controls.Input.Suggest>
    </div>
    <div class="demo-SuggestWrapper">
        <h3>Suggest</h3>
        <Controls.Input.Suggest
                bind:value="_suggest2Value"
                displayProperty="text">
            <ws:suggestTemplate>
                <div>
                    <Controls.Tabs.Buttons displayProperty="title" keyProperty="id">
                        <ws:items>
                            <ws:Array>
                                <ws:Object title="Сотрудники" id="Сотрудники"/>
                                <ws:Object title="Сотрудники" id="Сотрудники"/>
                            </ws:Array>
                        </ws:items>
                    </Controls.Tabs.Buttons>
                    <Controls.Container.List searchParam="title" source="{{_suggestSource}}">
                        <ws:navigation source="page" view="page">
                            <ws:sourceConfig pageSize="{{2}}" page="{{0}}" mode="totalCount"/>
                        </ws:navigation>
                        <ws:content>
                            <Controls:List
                                    displayProperty="title"
                                    idProperty="id">
                                <ws:emptyTemplate>
                                    Нет данных
                                </ws:emptyTemplate>
                            </Controls:List>
                        </ws:content>
                    </Controls.Container.List>
                </div>
            </ws:suggestTemplate>
            <ws:minSearchLength>
                <ws:Number>3</ws:Number>
            </ws:minSearchLength>
        </Controls.Input.Suggest>
    </div>
</div>
