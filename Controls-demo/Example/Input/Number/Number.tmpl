<div class="example-Number">
   <Controls-demo.Example.resource.BaseDemoInput title="Number input field">
      <ws:contents>
         <ws:Array>
            <ws:Object>
               <ws:template>
                  <div>
                     <div class="example-Base__specify">With tip in</div>
                     <Controls.Input.Number placeholder="Count"
                                            precision="{{0}}"
                                            bind:value="_value1"/>
                  </div>
               </ws:template>
            </ws:Object>
            <ws:Object spacing="normal">
               <ws:template>
                  <div>
                     <div class="example-Base__specify">Input of integers with a set range (0-999)</div>
                     <Controls.Input.Number placeholder="Count"
                                            precision="{{0}}"
                                            integersLength="{{3}}"
                                            bind:value="_value2"/>
                  </div>
               </ws:template>
            </ws:Object>
            <ws:Object spacing="normal">
               <ws:template>
                  <div>
                     <div class="example-Base__specify">Input of integer and fractional part</div>
                     <Controls.Input.Number placeholder="Count"
                                            precision="{{2}}"
                                            bind:value="_value3"/>
                  </div>
               </ws:template>
            </ws:Object>
         </ws:Array>
      </ws:contents>
   </Controls-demo.Example.resource.BaseDemoInput>
</div>