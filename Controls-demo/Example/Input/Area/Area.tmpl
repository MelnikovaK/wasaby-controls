<div class="example-Area">
   <Controls-demo.Example.resource.BaseDemoInput title="Multiline input field">
      <ws:contents>
         <ws:Array>
            <ws:Object>
               <ws:template>
                  <div>
                     <div class="example-Base__specify">Dynamic height</div>
                     <Controls.Input.Area placeholder="Helpful tip"
                                          maxLines="{{7}}"
                                          bind:value="_value1"/>
                  </div>
               </ws:template>
            </ws:Object>
            <ws:Object spacing="normal">
               <ws:template>
                  <div>
                     <div class="example-Base__specify">Fixed height</div>
                     <Controls.Input.Area placeholder="Helpful tip"
                                          minLines="{{4}}"
                                          maxLines="{{4}}"
                                          bind:value="_value2"/>
                  </div>
               </ws:template>
            </ws:Object>
         </ws:Array>
      </ws:contents>
   </Controls-demo.Example.resource.BaseDemoInput>
</div>