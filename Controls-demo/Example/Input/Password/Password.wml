<div class="example-Password">
   <Controls-demo.Example.resource.BaseDemoInput title="Password input field" blocker="{{true}}">
      <ws:contents>
         <ws:Array>
            <ws:Object>
               <ws:template>
                  <Controls.Input.Password placeholder="Password"
                                           revealable="{{false}}"
                                           bind:value="_value1"/>
               </ws:template>
            </ws:Object>
            <ws:Object spacing="normal">
               <ws:template>
                  <div class="ws-flexbox ws-flex-column">
                     <div class="example-Base__specify">Field with a button to toggle the visibility of the password</div>
                     <Controls.Input.Password placeholder="Password"
                                              revealable="{{true}}"
                                              bind:value="_value2"/>
                  </div>
               </ws:template>
            </ws:Object>
         </ws:Array>
      </ws:contents>
   </Controls-demo.Example.resource.BaseDemoInput>
</div>