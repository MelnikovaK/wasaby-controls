<Controls.grid:View
        attr:class="controlsDemo__wrapper controlsDemo_fixedWidth500"
        keyProperty="id"
        source="{{_viewSource}}"
        rowSeparatorVisibility="{{true}}"
        columns="{{_columns}}"
        groupingKeyCallback="{{_groupingKeyCallback}}">
    <ws:groupTemplate>
        <ws:partial template="Controls/grid:GroupTemplate" itemData="{{itemData}}" expanderAlign="right"/>
    </ws:groupTemplate>
</Controls.grid:View>
