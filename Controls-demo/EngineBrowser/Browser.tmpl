<Controls.EngineBrowser
        searchParam="title"
        source="{{_source}}"
        filterButtonSource="{{_items}}"
        fastFilterSource="{{_fastFilterSource}}"
        class="demo-browser">
   <ws:search>
      <Controls.Input.Search bind:value="value"
                             class="demo-browser-search"/>
   </ws:search>

   <ws:topTemplate>
      <div class="demo-browser-topTemplate">
         <Controls.Button icon="icon-24 icon-AddButtonNew" style="iconButtonBordered" class="demo-browser-button"/>
         <Controls.Button icon="icon-24 icon-Unfavourite" size="s" style="linkMain" class="demo-browser=button"/>
      </div>
   </ws:topTemplate>
   <ws:fastFilter>
      <Controls.Filter.Fast/>
   </ws:fastFilter>
   <ws:filterButton>
      <Controls.Filter.Button template="tmpl!Controls-demo/EngineBrowser/resources/filterButtonEngineTemplate"/>
   </ws:filterButton>
   <ws:content>
      <Controls.List
              keyProperty="id"
              emptyTemplate="Не найдено">
         <ws:itemTemplate>
            <ws:partial template="tmpl!Controls/List/ItemTemplate">
               <ws:contentTemplate>
                  <div class="demo-browser-list">
                     <div>{{itemTemplate.itemData.item.get('title')}}</div>
                     <div>{{itemTemplate.itemData.item.get('country')}}</div>
                  </div>
               </ws:contentTemplate>
            </ws:partial>
         </ws:itemTemplate>
      </Controls.List>
   </ws:content>
</Controls.EngineBrowser>