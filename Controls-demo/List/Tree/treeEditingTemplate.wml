<ws:partial template="Controls/grids:ColumnTemplate" itemData="{{itemData}}">
    <ws:contentTemplate>
        <ws:partial template="Controls/lists:EditingTemplate"
                    value="{{ itemData.item[itemData.column.displayProperty || 'title'] }}"
                    itemData="{{itemData}}"
                    enabled="{{true}}">
            <ws:editorTemplate>
                <Controls.Input.Text bind:value="itemData.item[itemData.column.displayProperty || 'title']"
                                     selectOnClick="{{ false }}" />
            </ws:editorTemplate>
        </ws:partial>
    </ws:contentTemplate>
</ws:partial>