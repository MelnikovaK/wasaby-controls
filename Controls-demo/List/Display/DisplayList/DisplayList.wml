         <ws:partial
            template="wml!Controls-demo/List/Display/DisplayList/resources/For"
            collection="{{ _collection }}"
            keyPrefix="{{ _templateKeyPrefix }}" />
      </div>
   </ws:if>
   <ws:else>
      <!-- TODO -->
      <div>Sorry, no data :(</div>
   </ws:else>
</div>
