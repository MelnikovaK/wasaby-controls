<div class="controlsDemo__wrapper">
    <Controls.list:View keyProperty="id" source="{{_viewSource}}">
        <ws:itemTemplate>
            <ws:partial template="Controls/list:ItemTemplate" displayProperty="value" clickable="{{itemTemplate.itemData.item.get('clickable')}}" highlightOnHover="{{itemTemplate.itemData.item.get('hovered')}}" />
        </ws:itemTemplate>
    </Controls.list:View>
</div>
