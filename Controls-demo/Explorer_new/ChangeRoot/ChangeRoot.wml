<div class="controlsDemo__wrapper">
        <h4>Current root: {{'' + _root}}</h4>
        <Controls.explorer:View
            source="{{_viewSource}}"
            bind:root="_root"
            attr:class="demo-Explorer"
            keyProperty="id"
            displayProperty="title"
            parentProperty="parent"
            nodeProperty="parent@"
            viewMode="{{_viewMode}}"
            header="{{_gridHeader}}"
            columns="{{_columns}}">
        </Controls.explorer:View>
</div>
