<div class="ControlsDemo-EditAtPlace__template">
   <div class="ControlsDemo-EditAtPlace__fieldWrapper">
      <span>Kind:</span>
      <Controls.EditAtPlace.EditAtPlaceTemplate
         bind:value="editObject.title"
         isEditing="{{isEditing}}"
      >
         <ws:editorTemplate>
            <Controls.Input.Text selectOnClick="{{ false }}" />
         </ws:editorTemplate>
      </Controls.EditAtPlace.EditAtPlaceTemplate>
   </div>
   <div class="ControlsDemo-EditAtPlace__fieldWrapper">
      <span>Created:</span>
      <Controls.EditAtPlace.EditAtPlaceTemplate
         bind:value="editObject.date"
         isEditing="{{isEditing}}"
      >
         <ws:editorTemplate>
            <Controls.Input.Text selectOnClick="{{ false }}" />
         </ws:editorTemplate>
      </Controls.EditAtPlace.EditAtPlaceTemplate>
   </div>
</div>