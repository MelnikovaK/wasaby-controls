<ws:template name="limit">
<Controls.Filter.Button.Panel.FilterSelect
        keyProperty="key"
        bind:textValue="item.textValue">
   <ws:items>
      <ws:Array>
         <ws:Object key="{{1}}" value="Due date"/>
         <ws:Object key="{{2}}" value="Overdue"/>
      </ws:Array>
   </ws:items>
</Controls.Filter.Button.Panel.FilterSelect>
