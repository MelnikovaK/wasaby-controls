         <ws:Object key="{{1}}" value="My"/>
         <ws:Object key="{{2}}" value="My department"/>
      </ws:Array>
   </ws:items>
</Controls.Filter.Button.Panel.FilterSelect>
</ws:template>

<ws:template name="unread">
<Controls.Filter.Button.Panel.FilterLink caption="Unread"/>
