<div attr:class="controls-BreadCrumbsView__items">
    <ws:for data="itemData in items">
        <ws:partial attr:class="{{!itemData.isDots && styledItemKey === itemData.item[keyProperty] ? 'controls-BreadCrumbsView__dropTarget'}}"
                    template="{{itemTemplate}}"
                    itemData="{{itemData}}"
                    on:mouseEnter="_onHoveredItemChanged(itemData.item[keyProperty])"
                    on:mouseLeave="_onHoveredItemChanged()"
                    on:click="_onItemClick(itemData)"/>
    </ws:for>
</div>