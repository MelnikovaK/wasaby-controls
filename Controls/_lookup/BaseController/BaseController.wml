<div>
   <ws:partial template="{{_options.content}}" items="{{_getItems()}}"
               on:showSelector="_onShowSelectorHandler()"
               on:addItem="_onAddItemHandler()"
               on:removeItem="_onRemoveItemHandler()"
               on:updateItems="_onUpdateItemsHandler()"
               on:valueChanged="_notifyHandler('valueChanged')"
               on:itemClick="_notifyHandler('itemClick')"
               on:inputCompleted="_notifyHandler('inputCompleted')"
               on:openInfoBox="_notifyHandler('openInfoBox')"
               on:closeInfoBox="_notifyHandler('closeInfoBox')"/>

   <ws:if data="{{_options.selectorTemplate}}">
      <Controls._lookup.Opener name="selectorOpener" template="{{_options.selectorTemplate.templateName}}" on:result="_selectCallback()" on:close="_closeHandler()"/>
   </ws:if>
</div>
