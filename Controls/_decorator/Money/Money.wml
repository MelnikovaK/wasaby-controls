<span class="controls-Money_theme_{{_options.theme}}" title="{{_options.title || _parsedNumber.number}}">
   <span class="controls-Money__integer_theme_{{_options.theme}}_style_{{_options.style}}">{{_parsedNumber.integer}}</span>
   <ws:if data="{{_isDisplayFractionPath(_parsedNumber.fraction, _options.displayEmptyDecimal)}}">
      <span class="controls-Money__fraction_theme_{{_options.theme}}">{{_parsedNumber.fraction}}</span>
   </ws:if>
</span>
