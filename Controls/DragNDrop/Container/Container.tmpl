<Controls.Event.Listener name="dragEndDetect" register="documentDragEnd" attr:fixCompatible="1">
   <Controls.Event.Listener name="dragStartDetect" register="documentDragStart" attr:fixCompatible="1">
      <ws:partial
              template="{{_options.content}}"
              scope="{{_options}}"
              attr:class="controls-DragNDrop"
              dragAvatarOptions="{{_avatarOptions}}"
              dragAvatarTemplate="{{_avatarTemplate}}"
              on:_updateDragAvatar="_updateDragAvatar()"
              on:_documentDragStart="_documentDragStart()"
              on:_documentDragEnd="_documentDragEnd()">
      </ws:partial>
   </Controls.Event.Listener>
</Controls.Event.Listener>