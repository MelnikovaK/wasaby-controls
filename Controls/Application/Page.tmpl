<html lang="ru" xml:lang="ru" xmlns="http://www.w3.org/1999/xhtml" key="1_" id="root" component="Controls/Application">
<head key="1_1_">
   <ws:if data="{{ _options.cssLinks }}">
      <ws:for data="index, value in cssLinks">
         <link rel="stylesheet" href="{{ value }}" type="text/css" key="{{ 'css_' + index }}"/>
      </ws:for>
   </ws:if>
   <title key="title">{{ _options.title }}</title>

   <link key="fonts-css-for-page" rel="stylesheet" data-pack-name="ws-core-styles" type="text/css" href="{{_options.wsRoot}}css/themes/webfonts/fonts.css" />
   <link key="core-min-css-for-page" rel="stylesheet" data-pack-name="ws-core-styles" type="text/css" href="{{_options.wsRoot}}css/core-min.css" />
   <meta name="viewport" content="width=device-width, initial-scale=1"/>
</head>
<!--Класс на body навешен чтобы в демках нормально работал старый ScrollContainer-->
<body class="ws-is-chrome" id="body" key="1_2_">
<ws:if data="{{ _options.content }}">
   <ws:partial template="{{ _options.content }}" scope="{{ _tplConfig }}"></ws:partial>
</ws:if>
<div key="scripts">
   <script key="ws-config">
      window.wsConfig = {
         wsRoot: '{{_options.wsRoot}}',
         resourceRoot: '{{_options.resourceRoot}}'
      };
   </script>
   <script key="config" type="text/javascript" src="{{_options.wsRoot}}ext/requirejs/bundles.js"></script>
   <script key="require" type="text/javascript" src="/cdn/requirejs/2.3.5-p1/require-min.js"></script>
   <script key="config" type="text/javascript" src="{{_options.wsRoot}}ext/requirejs/config.js"></script>
   <script key="contents" type="text/javascript" src="{{_options.resourceRoot}}/contents.js"></script>
   <ws:if data="{{ _options.jsLinks }}">
      <ws:for data="index, value in jsLinks">
         <ws:if data="{{ value.indexOf('core-init-min.js') === -1 }}">
            <script type="text/javascript" src="{{ value }}" defer="defer" key="{{ 'scripts_' + index }}"></script>
         </ws:if>
      </ws:for>
   </ws:if>
   <script>
      require(['Core/app']);
   </script>
</div>
</body>
</html>
