                        <!--scope="{{...}}"-->
               <!--&gt;-->
               <!--<ws:partial template="tmpl!Controls/Calendar/DateRangePickerItem"-->
                           <!--item="{{itemTemplate.itemData.item}}"-->
                           <!--startValue="{{startValue}}"-->
                           <!--endValue="{{endValue}}"-->

                           <!--_selectionProcessing="{{_selectionProcessing}}"-->
                           <!--_selectionBaseValue="{{_selectionBaseValue}}"-->
                           <!--_selectionHoveredValue="{{_selectionHoveredValue}}"-->

                           <!--startValueChangedHandler="{{startValueChangedHandler}}"-->
                           <!--endValueChangedHandler="{{endValueChangedHandler}}"-->

                           <!--selectionProcessingHandler="{{selectionProcessingHandler}}"-->
                           <!--selectionBaseValueHandler="{{selectionBaseValueHandler}}"-->
                           <!--selectionHoveredValueHandler="{{selectionHoveredValueHandler}}"-->
               <!--&gt;-->
               <!--</ws:partial>-->
         </ws:partial>
      </ws:itemTemplate>
   </Controls.List>
</Controls.Container.Scroll>