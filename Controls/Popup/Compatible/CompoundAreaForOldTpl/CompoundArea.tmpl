<div config="{{_options.__$config}}" data-component="{{_moduleName}}" class="{{_className}}" on:close="closeHandler()" on:keyup="_keyUp()">
   <ws:if data="{{_options.type === 'stack'}}">
      <Controls.Popup.Templates.Stack.StackTemplate caption="{{_options.caption || _options.title || _options.templateOptions.caption}}">
         <ws:contentArea>
            <div name="compoundBlock" attr:class="controls-CompoundArea__container"></div>
         </ws:contentArea>
      </Controls.Popup.Templates.Stack.StackTemplate>
   </ws:if>
   <ws:else>
      <Controls.Popup.Templates.Dialog.DialogTemplate
         caption="{{_options.caption || _options.title || _options.templateOptions.caption}}"
         hideCross="{{_options.hideCross}}">
         <ws:contentArea>
            <div name="compoundBlock" attr:class="controls-CompoundArea__container"></div>
         </ws:contentArea>
      </Controls.Popup.Templates.Dialog.DialogTemplate>
   </ws:else>
</div>