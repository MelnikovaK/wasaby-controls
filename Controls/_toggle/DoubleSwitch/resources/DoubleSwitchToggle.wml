<span class="controls-DoubleSwitch__toggle controls-DoubleSwitch__toggle_theme_{{_options.theme}}
            {{_enabled ? 'controls-DoubleSwitch__toggle_enabled'}}
            {{_orientation === 'vertical' ? 'controls-DoubleSwitch__toggle_vertical controls-DoubleSwitch__toggle_vertical_theme_' + (_options.theme)  :
             'controls-DoubleSwitch__toggle_horizontal controls-DoubleSwitch__toggle_horizontal_theme_' + (_options.theme)}}
            {{_value ? _orientation === 'vertical' ? 'controls-DoubleSwitch__toggle_vertical_checked' : 'controls-DoubleSwitch__toggle_horizontal_checked'}}"
      on:click="_clickToggleHandler()">
   <span class="{{_orientation === 'vertical' ? 'controls-DoubleSwitch__line_vertical controls-DoubleSwitch__line_vertical_theme_' + (_options.theme) :
                     'controls-DoubleSwitch__line_horizontal_theme_' + (_options.theme)}}
               {{_value ? _orientation === 'vertical' ? 'controls-DoubleSwitch__line_vertical_checked controls-DoubleSwitch__line_vertical_checked_theme_' + (_options.theme) :
               'controls-DoubleSwitch__line_horizontal_checked_theme_' + (_options.theme) :
               _orientation === 'vertical' ? 'controls-DoubleSwitch__line_vertical_notChecked_theme_' + (_options.theme) :
               'controls-DoubleSwitch__line_horizontal_notChecked_theme_' + (_options.theme) }}
               {{_enabled ? 'controls-DoubleSwitch__line_enabled controls-DoubleSwitch__line_enabled_theme_' + (_options.theme) :
               'controls-DoubleSwitch__line_disabled controls-DoubleSwitch__line_disabled_theme_' + (_options.theme)}}"></span>
   <ws:partial template="wml!Controls/_toggle/resources/SwitchCircle/SwitchCircle" readOnly="{{!_enabled}}" selected="{{true}}"/>
</span>
