<div class="controls-GroupRadio">
    <div class="controls-RadioGroup__wrapper_{{_options.direction}}">
        <ws:for data="item in _items">
            <ws:partial template="tmpl!Controls/Toggle/Radio/resources/ItemTemplate"
                        attr:class="{{ItemClass(item, _items)}}"
                        item="{{item}}"
                        readOnly="{{isEnabled()}}"
                        selected="{{item.get(_options.keyProperty) === _options.selectedKey}}"/>
        </ws:for>
    </div>
</div>