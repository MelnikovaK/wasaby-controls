<div class="controls-Label controls-Label_underline_{{_options.underline}}">
   <div class="controls-Label__wrapper">
      <div class="controls-Label__caption controls-Label__caption_underline_{{_options.underline}}
               controls-Label__caption_underline_{{_options.underline}}_{{_options.readOnly ? 'read' : 'edit'}}">
         <ws:partial template="{{_options.caption}}"/>
         <ws:if data="{{_options.href}}">
            <a class="controls-Label__fakeLink" href="{{_options.href}}"></a>
         </ws:if>
      </div>
      <ws:if data="{{_options.required}}">
         <div class="controls-Label__asterisk">&nbsp;*</div>
      </ws:if>
   </div>
</div>