<ws:if data="{{!readOnly}}">
   <input
      name="{{inputName || 'input'}}"
      type="{{ type }}"
      value="{{value}}"
      class="controls-InputRender__field {{isEdge ? 'controls-InputRender__field_isEdge'}}"
      autocomplete="{{autocomplete ? 'off' : 'new-password'}}"
      autocorrect="off"
   />
</ws:if>
<ws:else>
   <div class="controls-InputRender__field controls-InputRender__field_readOnly" tabindex="-1">{{value}}</div>
</ws:else>
