<ws:if data="{{_use}}">
   <div class="controls-InputBase__stretcher">
      <ws:partial attr:class="controls-InputBase__stretcher-content" template="{{content}}" value="{{value}}"/>
      <div class="controls-InputBase__stretcher-block">{{value || emptySymbol}}</div>
   </div>
</ws:if>
<ws:else>
   <ws:partial template="{{content}}" value="{{value}}"/>
</ws:else>