<div class="controls-PropertyGrid__editor-theme_{{theme}}" >
    <ws:if data="{{itemData.item.caption !== ''}}">
        <span class="controls-PropertyGrid__editor-caption-theme_{{theme}}">{{itemData.item.caption || itemData.item.name}}</span>
    </ws:if>
    <Controls.Container.Async templateName="{{itemData.item.editorTemplateName}}" templateOptions="{{itemData.item.editorOptions}}"/>
</div>