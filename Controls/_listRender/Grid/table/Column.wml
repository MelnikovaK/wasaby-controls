<td
   attr:class="{{ column.getCellClasses(highlightOnHover) }}"
   attr:style="{{ column.getCellStyles() }}">
   <ws:if data="{{ column.shouldDisplayMarker() }}">
      <div attr:class="{{ column.getMarkerClasses() }}"></div>
   </ws:if>
   <ws:partial
      template="{{ column.getTemplate() }}"
      item="{{ column }}" />
   <!-- valign, halign -->
</td
>
