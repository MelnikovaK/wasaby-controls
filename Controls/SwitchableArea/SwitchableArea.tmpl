<div class="controls-SwitchableArea">
    <ws:for data="item in _options.items">
        <ws:if data="{{_viewModel.mustBeLoad(item)}}">
            <ws:partial template="{{_options.itemTemplate || _defaultItemTemplate}}" currentItemId="{{getItemId(item)}}" item="{{item}}" _options="{{_options}}"/>
        </ws:if>
    </ws:for>
</div>