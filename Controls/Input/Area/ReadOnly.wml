<div name="readOnlyField"
     class="controls-Area__field_readOnly
            controls-Area__field_theme_{{options.theme}}_margin controls-Area__fieldWrapper">
   <ws:if data="{{value}}">
      <Controls.Decorator.WrapURLs if="{{value}}" text="{{value}}{{emptySymbol}}"/>
   </ws:if>
   <ws:else>{{emptySymbol}}</ws:else>
</div>