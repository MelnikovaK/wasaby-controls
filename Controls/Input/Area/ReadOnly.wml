<div name="readOnlyField"
     class="controls-Area__field_readOnly controls-Area__field_margin controls-Area__fieldWrapper">
   <ws:if data="{{value}}">
      <Controls.Decorator.WrapURLs attr:class="controls-Area__wrapURLs" if="{{value}}" text="{{value}}"/>
      <span class="controls-Area__emptySymbol">{{emptySymbol}}</span>
   </ws:if>
   <ws:else>{{emptySymbol}}</ws:else>
</div>