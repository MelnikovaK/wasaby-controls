<input
   name="input"
   type="{{ type }}"
   value="{{value}}"
   class="controls-InputRender__field"
   readonly="{{enabled ? '' : 'readonly'}}"
   autocomplete="off"
   required="true"
/>
<ws:partial
   template="tmpl!Controls/Input/resources/CustomPlaceholder"
   placeholder="{{ placeholder }}"
   attr:tabindex="{{-1}}"
/>