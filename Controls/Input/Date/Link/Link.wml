<div class="controls-DateLink">
   <Controls.Input.Date.LinkView
      scope="{{_options}}"
      bind:startValue="_options.value"
      bind:endValue="_options.value"
      on:linkClick="_openDialog()"
      attr:class="controls-DateLink__view"
   />

   <Controls.Popup.Opener.Sticky name="opener">
      <ws:popupOptions
         template="SBIS3.CONTROLS/Date/RangeBigChoose"
         closeByExternalClick="{{true}}"
      />
   </Controls.Popup.Opener.Sticky>
</div>
