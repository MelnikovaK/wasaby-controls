<div>
    <ws:partial template="{{contentTemplate}}"
                item="{{item}}"
                size="{{size || 'default'}}"
                style="{{style || 'default'}}"
                clickable="{{clickable}}"
                displayProperty="{{displayProperty}}"
    />
    <ws:if data="{{!readOnly}}">
        <ws:partial template="{{crossTemplate}}"/>
    </ws:if>
    <ws:if data="{{!isLastItem && itemsLayout !== 'twoColumns'}}">,</ws:if>
</div>