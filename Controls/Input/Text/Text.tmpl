<ws:Controls.Input.resources.TextRender.TextRender
   type="text"
   value="{{_value}}"
   enabled="{{isEnabled()}}"
   tagStyle="{{_options.tagStyle}}"
   placeholder="{{_options.placeholder}}"
   selectOnClick="{{_options.selectOnClick}}"
   getInputData="{{_getInputData}}"
   on:onChangeValue="_inputHandler()"
   on:onTagHover="_notifyHandler('onTagHover')"
   on:onTagClick="_notifyHandler('onTagClick')"
   on:onInputFinish="_notifyHandler('onInputFinish')"
/>