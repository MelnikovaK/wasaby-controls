
<div class="ws-ListView" style="position: relative;">
   <div class="ws-ListView__topLoadTrigger"></div>
   <div class="ws-ListView__items">
      <ws:for data="_getStartEnumerationPosition(); _checkConditionForEnumeration(); _getNextEnumerationPosition();">
         <ws:partial
               template="{{_itemTemplate}}"
               itemData="{{_listModel.getCurrent()}}"/>
      </ws:for>
   </div>
   <div class="ws-ListView__bottomLoadTrigger"></div>
</div>