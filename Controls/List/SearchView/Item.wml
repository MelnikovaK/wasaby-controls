<ws:if data="{{itemData.item.getId}}">
   <ws:partial template="wml!Controls/List/Grid/Item"
               itemData="{{itemData}}"
               multiSelectTpl="{{multiSelectTpl}}"/>
</ws:if>
<ws:else>
   <ws:partial template="wml!Controls/List/TreeGridView/Item"
               itemData="{{itemData}}"
               multiSelectTpl="{{multiSelectTpl}}"
               colspan="{{true}}"
               on:click="_onSearchItemClick(itemData.dispItem)">
      <ws:colspanTemplate>
         <Controls.BreadCrumbs displayProperty="{{itemData.displayProperty}}" items="{{colspanTemplate.itemData.item}}" on:itemClick="_onSearchPathClick()" keyProperty="{{itemData.keyProperty}}">
            <ws:itemTemplate>
               <ws:partial template="wml!Controls/BreadCrumbs/View/resources/itemTemplate">
                  <ws:contentTemplate>
                     <ws:partial template="wml!Controls/List/SearchView/SearchBreadCrumbsContent" searchValue="{{itemData.searchValue}}"/>
                  </ws:contentTemplate>
               </ws:partial>
            </ws:itemTemplate>
         </Controls.BreadCrumbs>
      </ws:colspanTemplate>
   </ws:partial>
</ws:else>