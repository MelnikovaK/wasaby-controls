<div class="controls-PropertyGrid">
   <ws:for data="index, item in _options.items">
      <div if="{{item.visibility}}" class="controls-PropertyGrid__item">
         <ws:partial template="{{item.editor}}" item="{{item}}" on:valueChanged="_valueChangedHandler()"
                     on:selectedKeysChanged="_valueChangedHandler(index)"/>
      </div>
   </ws:for>
</div>