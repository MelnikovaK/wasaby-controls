<a attr:class="controls-Lookup__link {{_options.readOnly ? 'controls-Lookup__link_readOnly' : 'controls-Lookup__link-default'}}"
   attr:title="{{_options.tooltip}}"
   on:click="_clickHandler()"
   on:keypress="_keyUpHandler()">
    <span attr:class="controls-Lookup__link__content">
        <span attr:class="controls-Lookup__link__text">{{_options.caption}}</span>
    </span>
</a>